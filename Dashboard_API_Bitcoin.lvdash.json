  "datasets": [
    {
      "name": "d678192d",
      "displayName": "preco_minimo",
      "queryLines": [
        " SELECT \r\n",
        "  round(valor_brl,2)  AS min_price,\r\n",
        "  timestamp  AS min_timestamp\r\n",
        "\r\n",
        " FROM pipeline_api_bitcoin.biticoin_delta.btc_data\r\n",
        " WHERE criptomoeda =  'BTC'  \r\n",
        " ORDER BY valor_brl ASC,timestamp DESC\r\n",
        " LIMIT 1"
      ]
    },
    {
      "name": "af88a87d",
